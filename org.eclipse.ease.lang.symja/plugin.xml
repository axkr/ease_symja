<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            id="org.eclipse.ease.lang.symja.sym"
            name="org.eclipse.ease.lang.symja.sym"
            base-type="org.eclipse.core.runtime.text"
            file-extensions="sym">
      </content-type>
   </extension>
      <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Symja Editor"
            icon="icons/obj16/java.png"
            extensions="sym"
            contributorClass="org.eclipse.ease.lang.symja.symjaeditor.SymjaActionContributor"
            class="org.eclipse.ease.lang.symja.symjaeditor.SymjaEditor"
            id="org.eclipse.ui.JavaEditor">
      </editor>
   </extension> 
      <extension
         point="org.eclipse.core.filebuffers.documentSetup">
      <participant
            class="org.eclipse.ease.lang.symja.ValidatorDocumentSetupParticipant"
            contentTypeId="org.eclipse.ease.lang.symja.sym"
            extensions="sym">
      </participant> 
   </extension> 
    
   <extension
         point="org.eclipse.ui.genericeditor.reconcilers">
      <reconciler
            class="org.eclipse.ease.lang.symja.SymReconciler"
            contentType="org.eclipse.ease.lang.symja.sym">
      </reconciler>
   </extension>
   <extension
         point="org.eclipse.ease.language">
      <engine
            class="org.eclipse.ease.lang.symja.SymjaReplScriptEngine"
            id="org.eclipse.ease.lang.symja.engine"
            name="Symja"
            priority="10">
      </engine>
      <launchExtension
            class="org.eclipse.ease.lang.symja.SymjaEnvironementBootStrapper"
            engineID="org.eclipse.ease.engine.symja">
      </launchExtension>
   </extension>
</plugin>
